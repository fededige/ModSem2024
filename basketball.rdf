    <!-- http://schema.org/StadiumOrArena -->

    <owl:Class rdf:about="http://schema.org/StadiumOrArena">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ModSem/Basketball#Palazzetto"/>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ModSem/Basketball#ospitaPartita"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ModSem/Basketball#Partita"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ModSem/Basketball#èPossedutoDaSquadra"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ModSem/Basketball#Squadra"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ModSem/Basketball#situatoIn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://schema.org/City"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
    </owl:Class>
    


