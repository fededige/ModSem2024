                <owl:someValuesFrom rdf:resource="http://schema.org/StadiumOrArena"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
    </owl:Class>
    


